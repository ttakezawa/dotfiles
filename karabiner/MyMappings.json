{
  "title": "My Mappings",
  "rules": [
    {
      "description": "Map option+← to Home",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_arrow",
            "modifiers": {
              "mandatory": ["right_alt"]
            }
          },
          "to": [
            {
              "key_code": "home"
            }
          ]
        }
      ]
    },
    {
      "description": "Map option+→ to End",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_arrow",
            "modifiers": {
              "mandatory": ["right_alt"]
            }
          },
          "to": [
            {
              "key_code": "end"
            }
          ]
        }
      ]
    },
    {
      "description": "Map option+↑ to page_up",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "up_arrow",
            "modifiers": {
              "mandatory": ["right_alt"]
            }
          },
          "to": [
            {
              "key_code": "page_up"
            }
          ]
        }
      ]
    },
    {
      "description": "Map option+← to page_down",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "down_arrow",
            "modifiers": {
              "mandatory": ["right_alt"]
            }
          },
          "to": [
            {
              "key_code": "page_down"
            }
          ]
        }
      ]
    },
    {
      "description": "Map ctrl+[ to escape",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": ["control"],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "key_code": "escape"
            }
          ]
        }
      ]
    },
